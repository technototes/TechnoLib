{
  "name": "TechnoLib",
  "version": "1.0.0",
  "description": "TechnoLib automation scripts",
  "main": "build/automation.js",
  "repository": "https://github.com/technototes/TechnoLib.git",
  "author": "Kevin Frei <kevinfrei@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "run-script-os",
    "build:win32": ".\\gradlew build",
    "build:default": "./gradlew build",
    "fulldoc": "yarn run doc && yarn run format",
    "docs": "yarn run doc",
    "doc": "run-script-os",
    "doc:win32": ".\\gradlew javadoc",
    "doc:default": "./gradlew javadoc",
    "format": "prettier --write docs/**/*.js docs/**/*.html docs/**/*.css **/*.md Path/**/*.java RobotLibrary/**/*.java Vision/**/*.java"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "prettier-plugin-java": "^2.0.0",
    "run-script-os": "^1.1.6"
  }
}
